{
  "files": [
    {
      "pattern": "testsdata/a/a(.*)",
      "target": "${REPO1}",
      "flat": "true",
      "recursive": "true",
      "excludePatterns": [".*a2.*", ".*a3.in", null],
      "regexp": "true"
    },
    {
      "pattern": "testsdata/a/b/b(.*)",
      "target": "${REPO1}",
      "excludePatterns": [null, ".*b2.*", ".*b3.in"],
      "regexp": "true"
    },
    {
      "pattern": "testsdata/a/b/c/(.*)",
      "target": "${REPO1}",
      "flat": "true",
      "recursive": "true",
      "excludePatterns": ["testsdata/a/b/c/c1.in"],
      "regexp": "true"
    }
  ]
}